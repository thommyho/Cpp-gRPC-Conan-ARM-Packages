toolchain=/usr/aarch64-linux-gnu
target_host=aarch64-linux-gnu
cc_compiler=gcc
cxx_compiler=g++

[settings]
os=Linux
arch=armv8
compiler=gcc
compiler.version=9
compiler.libcxx=libstdc++11
compiler.cppstd=11
build_type=Release

[env]
CONAN_CMAKE_FIND_ROOT_PATH=$toolchain
CONAN_CMAKE_SYSTEM_NAME=Linux
CONAN_CMAKE_SYSTEM_PROCESSOR=aarch64
CONAN_CMAKE_FIND_ROOT_PATH_MODE_PROGRAM=Never

CHOST=$target_host
AR=$target_host-ar
AS=$target_host-as
CC=$target_host-$cc_compiler
CXX=$target_host-$cxx_compiler
LD=$target_host-ld
NM=$target_host-nm
OBJCOPY=$target_host-objcopy
OBJDUMP=$target_host-objdump
RANLIB=$target_host-ranlib
STRIP=$target_host-strip
PKG_CONFIG_PATH=/usr/lib/$toolchain/pkgconfig

CFLAGS=-march=armv8-a
CXXFLAGS=-march=armv8-a